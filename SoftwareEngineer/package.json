{
  "name": "softwareengineer",
  "private": true,
  "main": "main.js",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vue-tsc -b && vite build && electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.7.7",
    "element-plus": "^2.8.4",
    "emoji-regex": "^10.4.0",
    "github-markdown-css": "^5.7.0",
    "highlight.js": "^11.10.0",
    "js-audio-recorder": "^1.0.7",
    "markdown-it": "^14.1.0",
    "pinia": "^2.2.4",
    "typewriter-effect": "^2.21.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vue": "^3.5.10",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.5.12",
    "concurrently": "^8.0.0",
    "electron": "^32.1.2",
    "electron-builder": "^24.6.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.example.softwareengineer",
    "productName": "SoftwareEngineerApp",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons"
    }
  }
}
